length = float(input("输入长方体的长："))
width = float(input("输入长方体的宽："))
height = float(input("输入长方体的高："))

area = 2 *(length * width + length * height + width * height)
volume = length * width * height
print("长方体的面积为：%f"%area)
print("长方体的体积为：%f"%volume)





